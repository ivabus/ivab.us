[
  {
    "uri":"/",
    "alias": {
      "url":  "https://ivabus.dev"
    }
  },
  {
    "uri":"/",
    "alias": {
      "file": "src/main.rs"
    },
    "agent": {
      "regex": "^curl/[0-9].[0-9].[0-9]$"
    }
  },
  {
    "uri":"text",
    "alias": {
      "html": "sometext"
    },
    "agent": {
      "regex": "^curl/[0-9].[0-9].[0-9]$",
      "only_matching": true
    }
  },
  {
    "uri": "external",
    "alias": {
      "external": {
        "url": "https://iva.bz",
        "headers": {
          "user-agent": "curl/8.6.0"
        }
      }
    }
  }
]
